<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <AssemblyName>ArenaBuilds</AssemblyName>
    <Description>Server mod that lets you instantly equip custom builds via in-game commands</Description>
    <Version>0.2.11</Version>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <LangVersion>latest</LangVersion>
    <RootNamespace>ArenaBuilds</RootNamespace>
    <Authors>Crostomos</Authors>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="BepInEx.Unity.IL2CPP" Version="6.0.0-be.735" IncludeAssets="compile" />
    <PackageReference Include="BepInEx.Core" Version="6.0.0-pre.1" IncludeAssets="compile" />
    <PackageReference Include="BepInEx.PluginInfoProps" Version="2.1.0" />
    <PackageReference Include="VRising.Unhollowed.Client" Version="1.1.8.9179701" />
    <PackageReference Include="VRising.VampireCommandFramework" Version="0.10.3" />
  </ItemGroup>
</Project>